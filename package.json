{
  "name": "gulp-tasks-registrator",
  "version": "0.2.2",
  "description": "Registers gulp tasks from folders",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint -c ./.eslintrc.json src",
    "test": "npm run lint && mocha --reporter spec --require babel-register --recursive test",
    "build": "npm run test && npm run clean && babel src --out-dir lib"
  },
  "keywords": [
    "gulp",
    "task",
    "manager",
    "injector",
    "loader"
  ],
  "author": "Tim Voronov <ziflex@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ziflex/gulp-tasks-registrator.git"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.1",
    "glob": "^7.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-mocha": "^2.0.0",
    "estraverse-fb": "^1.3.1",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  }
}
